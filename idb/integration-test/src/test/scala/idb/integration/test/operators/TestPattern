03									{}          remove (a)
04									{}          update (a, b)
05									{a}         add (b)
06									{b}         add (a)
07									{a}         update (a, b)
08									{b}         update (b, a)
09									{c}			update (c, a)				|||||||||||||||||||||||||||||||||||||||||||||
10									{a}			update (a, c)               |||||||||||||||||||||||||||||||||||||||||||||
11									{a}         remove (a)
12 Add-double element  				{a}         add (a)
13									{c}         add (c)						||||||||||||||||||||||||||||||||||||||||||||
14									{a, a}      update (a, b)
15									{a, a}      remove (a)
16 Non-empty result    				{a, b}      remove (a)
17 Non-empty result other  		   	{a, b}      remove (b)
18									{a, a, b}	update (a, b)
19									{a, a, b}	remove (a)
20 Remove twice						{a, b}		remove (a)
												remove (b)
21 Re-add							{a}			remove (a)
